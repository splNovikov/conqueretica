    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4",
    "husky": "7.0.4",
    "prettier": "2.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/index.tsx",
      "!**/reportWebVitals.ts",
      "!**/react-app-env.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 93,
        "branches": 93,
        "functions": 97,
        "lines": 93
      }
    }
  }
}
